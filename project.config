[project]
	description = NFS-Ganesha, a NFSv3/NFSv4.x/9P file server in User Space
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Philippe DENIEL (phdeniel)
[access "refs/for/refs/*"]
	read = group user/Philippe DENIEL (phdeniel)
	push = group user/Philippe DENIEL (phdeniel)
	label-Code-Review = -2..+2 group user/Philippe DENIEL (phdeniel)
	label-Verified = -1..+1 group user/Philippe DENIEL (phdeniel)
	rebase = group user/Philippe DENIEL (phdeniel)
	removeReviewer = group user/Philippe DENIEL (phdeniel)
	submit = group user/Philippe DENIEL (phdeniel)
[access "refs/heads/*"]
	read = group user/Philippe DENIEL (phdeniel)
	create = group user/Philippe DENIEL (phdeniel)
	push = +force group user/Philippe DENIEL (phdeniel)
	pushMerge = group user/Philippe DENIEL (phdeniel)
[access "refs/tags/*"]
	push = +force group user/Philippe DENIEL (phdeniel)
	pushTag = group user/Philippe DENIEL (phdeniel)
	pushSignedTag = group user/Philippe DENIEL (phdeniel)
